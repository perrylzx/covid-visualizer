{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel -d ./build ./src -s --copy-files",
    "clean": "rm -rf build",
    "start": "npm run build && node ./build/bin/www",
    "start:dev": "nodemon --exec babel-node ./src/bin/www"
  },
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "download-git-repo": "^3.0.2",
    "express": "^4.16.4",
    "http-errors": "~1.7.3",
    "jade": "~1.11.0",
    "morgan": "~1.10.0",
    "papaparse": "^5.2.0",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/node": "^7.10.1",
    "nodemon": "^2.0.3"
  }
}
